  <filter-mapping>
    <filter-name>FirstFilter</filter-name>
    <servlet-name>LoginServ</servlet-name>
    <dispatcher>REQUEST</dispatcher>
  </filter-mapping>
  <filter>
    <display-name>Second_Filter</display-name>
    <filter-name>Second_Filter</filter-name>
    <filter-class>mypack.Second_Filter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Second_Filter</filter-name>
    <url-pattern>/Second_Filter</url-pattern>
  </filter-mapping>
